{
  "manifest_version": 3,
  "name": "CPM extensions",
  "description": "extension hỗ trợ click nhanh",
  "version": "1.0.0",
  "permissions": ["scripting", "activeTab"],

  "background": {
    "service_worker": "background.js"
  },

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  }
}
